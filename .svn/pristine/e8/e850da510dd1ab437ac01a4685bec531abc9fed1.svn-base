	src="js/vendor/bootstrap/bootstrap-datepicker.js"></script>	
<script type='text/javascript' src="js/vendor/alertify.js-0.3/lib/alertify.js"></script>
<script type='text/javascript' src="js/vendor/ckeditor/ckeditor.js"></script>
<script type='text/javascript' src="js/vendor/ng-table.js"></script>
<script type="text/javascript" src="js/vendor/jsPDF-master/jspdf.js"></script>
<script type="text/javascript" src="js/vendor/jsPDF-master/jspdf.plugin.from_html.js"></script>
<script type="text/javascript" src="js/vendor/jsPDF-master/jspdf.plugin.standard_fonts_metrics.js"></script>
<script type="text/javascript" src="js/vendor/jsPDF-master/jspdf.plugin.split_text_to_size.js"></script>
<script type="text/javascript" src="js/vendor/jsPDF-master/jspdf.plugin.addimage.js"></script>

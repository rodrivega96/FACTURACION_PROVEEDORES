<!-- INICIO JS APP -->
<script type='text/javascript' src="protected/configApp.js"></script>
<script type='text/javascript' src="protected/app.js"></script>
<script type='text/javascript' src="protected/controllers.js"></script>
<script type='text/javascript' src="protected/file/FileModel.js"></script>
<script type='text/javascript' src="protected/file/FileController.js"></script>
<script type='text/javascript' src="protected/user/UserModel.js"></script>
<script type='text/javascript' src="protected/user/UserController.js"></script>
<script type='text/javascript' src="protected/factura/FacturaController.js"></script>
<script type='text/javascript' src="protected/factura/FacturaModel.js"></script>
<script type='text/javascript' src="protected/autorizacion/AutorizacionController.js"></script>
<script type='text/javascript' src="protected/autorizacion/AutorizacionModel.js"></script>
<script type='text/javascript' src="protected/workflow/DetalleWFController.js"></script>
<script type='text/javascript' src="protected/workflow/DetalleWFModel.js"></script>

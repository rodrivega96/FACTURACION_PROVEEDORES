<body>
	<div ng-controller="MainMenuController">
		<!--[if lt IE 7]>
<p class="chromeframe">Se esta utilizando un navegador <strong>desactualizado</strong>. Por favor <a
        href="http://browsehappy.com/"> actualice su navegador</a> o <a
        href="http://www.google.com/chromeframe/?redirect=true"> active Google Chrome Frame</a> para mejorar su
    experiencia.</p>
<![endif]-->

		<div class="container">

			<div class="navbar navbar-default" role="navigation">

				<div class="container-fluid">

					<div class="navbar-header">
						<button type="button" class="navbar-toggle" data-toggle="collapse"
							data-target="#main-menu">
							<span class="sr-only">Toggle navigation</span> <span
								class="icon-bar"></span> <span class="icon-bar"></span> <span
								class="icon-bar"></span>
						</button>

						<a href="#">
							<img id="logoId" class="img-responsive" style="margin-right:18px;" src="img/logo_vates2.png" />
						</a>

					</div>

					<div class="navbar-collapse collapse" id="main-menu">

						<ul class="nav navbar-nav">
							<li><a href="#">AFP</a></li>
							<li class="dropdown" ng-repeat="firstLevelMenu in items"><a
								href="#" class="dropdown-toggle" data-toggle="dropdown">{{firstLevelMenu.title}}
									<b class="caret"></b>
							</a>
								<ul class="dropdown-menu">
									<li ng-repeat="secondLevelMenu in firstLevelMenu.subOptions">
										<a href="#{{secondLevelMenu.page}}">{{secondLevelMenu.title}}</a>
									</li>
								</ul></li>

						</ul>


						<ul class="nav navbar-nav navbar-right">
							<li ng-show="{{!isLoged}}"><a ng-click="openLogin()"
								href="#login-form">Iniciar Sesión</a></li>
							<li ng-show="{{isLoged}}"><a href="#logout">Cerrar
									Sesión</a> <label>Usuario: {{userName}}</label></li>
						</ul>
						<ul class="dropdown-menu">
							<li><a href="#loguot">Cerrar Sesión</a></li>
						</ul>

					</div>
					<div id="breadcrumbs">
						<ol class="breadcrumb">
							<li
								ng-repeat="breadcrumb in breadcrumbs.get() track by breadcrumb.path"
								ng-class="{ active: $last }"><a ng-if="!$last"
								ng-href="#{{ breadcrumb.path }}" ng-bind="breadcrumb.label"></a>
								<span ng-if="$last" ng-bind="breadcrumb.label"></span></li>
						</ol>
					</div>

					<!-- MODAL LOGIN -->
					<div id="modalLoginId" ng-show="modalLoginPass"
						class="ng-modal-dialog" ng-controller="LoginController">
						<div class="modal-content" ng-show="modalLogin">
							<div class="modal-header">
								<button type="button" ng_click="closeLogin()" class="close"
									data-dismiss="modal">&times;</button>
								<h4 class="modal-title">Inicio de Sesión</h4>
							</div>
							<div class="modal-body">
								<div class="text-center">
									<form name="form-login" id="formLoginId" ng-submit="loginFn()">
										<table class="table table-striped table-bordered table-hover">
											<tr>
												<td width="40%" align="left"><label name="name">Nombre
														de Usuario</label>
												<td align="left"><input class="max-width"
													id="inptNameId" type="text" name="name"
													ng-model="user.userName" required tabindex="1" />
											</tr>
											<tr>
												<td width="40%" align="left"><label name="name">Contraseña</label>
												<td align="left"><input class="max-width"
													type="password" name="name" ng-model="user.password"
													required tabindex="2" />
											</tr>
											<tr>
												<td width="20%" align="left"><label name="name">Dominio</label>
												<td align="left"><select class="max-width" name="domain"
													ng-model="valueDomain" id="idDomain" tabindex="3">
														<option value="{{domain.id}}"
															ng-repeat="domain in domains">{{domain.name}}</option>
												</select>
											</tr>
										</table>
										<div align="left">
											<a href="" ng-click="resetPass()">Recuperar contraseña</a>
										</div>

										<div style="display: none;" id="messageId" align="right">
											<label id="mjeErrorId" style="color: red;">Usuario
												y/o Contraseña Incorrecto</label>
										</div>
										<div class="modal-footer">
											<button type="button" ng_click="closeLogin()"
												class="btn btn-default" data-dismiss="modal" tabindex="5">Cerrar</button>
											<input type='submit' class="btn btn-primary" value="Aceptar" tabindex="4" />
										</div>
									</form>
								</div>
							</div>
						</div>

						<!-- RESET PASSWORD -->
						<div ng-show="modalPass">
							<div class="modal-header">
								<button type="button" ng_click="closeLogin()" class="close"
									data-dismiss="modal">&times;</button>
								<h4 class="modal-title">Recuperar Contraseña</h4>
							</div>
							<div class="modal-body">
								<div class="text-center">
									<form name="form-reset" ng-submit="resetFn()">
										<table class="table table-striped table-bordered table-hover">
											<tr>
												<td width="40%" align="left"><label name="name">Nombre
														de Usuario</label></td>
												<td align="left"><input class="max-width" name="name"
													ng-model="user.userName" required tabindex="1" /></td>
											</tr>
											<tr>
												<td width="40%" align="left"><label name="name">E-mail
														de Usuario</label></td>
												<td align="left"><input class="max-width"
													id="inptNameId" type="text" name="name"
													ng-model="user.userMail" required tabindex="2" /></td>
											</tr>
										</table>


										<div align="left">
											<a href="" ng-click="closeResetPass()">Ya tengo mi
												contraseña</a>
										</div>
										<!-- div ng-show="messageErrReset" align="right">
										<label style="color: red;">Datos Incorrectos</label>
									</div-->

										<div class="modal-footer">
											<button type="button" ng_click="closeLogin()"
												class="btn btn-default" data-dismiss="modal">Cerrar</button>
											<input type='submit' class="btn btn-primary" value="Aceptar" />
										</div>
									</form>
								</div>
							</div>
						</div>
						<!-- /.modal-content -->
					</div>
					<!-- /.modal-dialog -->

					<!-- MODAL WAIT -->
					<loading ng-show="isLoading">
					<div id="waitModalId" class="ng-modal-dialog"
						style="position: fixed; width: 100%; height: 120%; opacity: 0.4; filter: alpha(opacity = 40); background-color: #7B7B7B">
					</div>
					<div style="position: fixed; top: 40%; left: 40%;">
						<h4>
							<b>Procesando</b>
						</h4>
					</div>
					<div style="position: fixed; top: 37%; left: 54%;">
						<a href="#"><img class="img-responsive" src="img/loader.gif" /></a>
					</div>
					</loading>

				</div>
				<!-- /.container-fluid -->
				<!-- Main component for a primary marketing message or call to action -->
				<div class="jumbotron" id="main">

					<!-- Aquí inyectamos las vistas -->
					<div data-ng-view class="table-responsive" />

				</div>

			</div>


			<footer>
				<div class="container">
					<div class="text-center text-footer">
						<div>
							<span>Copyright © 2014 - All rights reserved | Vates S.A.
								- Software Development Company | Versión {{myAppVersion}}</span>
						</div>
					</div>
				</div>
			</footer>
		</div>
	</div>
</body>
</html>
